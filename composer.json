{
    "name": "nonoesp/authenticate",
    "description": "A Laravel helper package for frequently-used operations—so you don't have to think.",
    "license": "MIT",
    "authors": [{
        "name": "Nono Martínez Alonso",
        "email": "mundowarezweb@gmail.com"
    }],   
    "autoload": {
        "classmap": [
            "src/controllers",
            "src/facades"
        ],
        "psr-4": {
            "Nonoesp\\Authenticate\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "illuminate/support": "^6.0",
        "nonoesp/thinker": "^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nonoesp\\Authenticate\\AuthenticateServiceProvider"
            ],
            "aliases": {
                "Authenticate": "Nonoesp\\Authenticate\\Facades\\Authenticate"
            }
        }
    }
}